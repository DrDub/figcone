@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(figcone_tree)

if(FIGCONE_USE_NAMEOF)
    find_dependency(nameof)
endif()

if (NOT DEFINED FIGCONE_USE_ALL)
    if(FIGCONE_USE_JSON OR FIGCONE_USE_YAML OR FIGCONE_USE_TOML OR FIGCONE_USE_INI OR FIGCONE_USE_XML)
        set(FIGCONE_USE_ALL OFF)
    else()
        set(FIGCONE_USE_ALL ON)
    endif()
endif()

if(FIGCONE_USE_ALL OR FIGCONE_USE_JSON)
    find_dependency(figcone_json)
endif()
if(FIGCONE_USE_ALL OR FIGCONE_USE_YAML)
    find_dependency(figcone_yaml)
endif()
if(FIGCONE_USE_ALL OR FIGCONE_USE_TOML)
    find_dependency(figcone_toml)
endif()
if(FIGCONE_USE_ALL OR FIGCONE_USE_INI)
    find_dependency(figcone_ini)
endif()
if(FIGCONE_USE_ALL OR FIGCONE_USE_XML)
    find_dependency(figcone_xml)
endif()
include("${CMAKE_CURRENT_LIST_DIR}/figconeTargets.cmake")